<script type="text/javascript">

    var clockID = 0;
    function UpdateClock() {
        if (clockID) {
            clearTimeout(clockID);
            clockID = 0;
        }

        var txtDate = document.getElementById('dnn_idWaktu_txtClock1').value;
        var tDate = new Date(txtDate);
	
        var tscnd = tDate.getSeconds();
        tscnd = tscnd + 1;
        tDate.setSeconds(tscnd);
        var sDate = "";
        if (((tDate.getMinutes()) > 9) && ((tDate.getSeconds()) <= 9)) {
            document.getElementById('theTime').innerHTML = sDate 
                        + tDate.getHours("HH")+ ":"
                        + tDate.getMinutes() + ":0"
                        + tDate.getSeconds();
        } else if (((tDate.getMinutes()) > 9) && ((tDate.getSeconds()) > 9)) {
            document.getElementById('theTime').innerHTML = sDate 
                        + tDate.getHours("HH")+ ":"
                        + tDate.getMinutes() + ":"
                        + tDate.getSeconds();

        } else if (((tDate.getMinutes()) <= 9) && ((tDate.getSeconds()) <= 9)) {
            document.getElementById('theTime').innerHTML = sDate 
                        + tDate.getHours("HH")+ ":0"
                        + tDate.getMinutes() + ":0"
                        + tDate.getSeconds();
        } else {
            document.getElementById('theTime').innerHTML = sDate 
                        + tDate.getHours("HH")+ ":0"
                        + tDate.getMinutes() + ":"
                        + tDate.getSeconds();
        }

        document.getElementById('dnn_idWaktu_txtClock1').value = tDate;
        clockID = setTimeout("UpdateClock()", 1000);
    }
    function StartClock() {
        clockID = setTimeout("UpdateClock()", 100);
    }
    function KillClock() {
        if (clockID) {
            clearTimeout(clockID);
            clockID = 0;
        }
    }
</script>
